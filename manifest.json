{
 "name": "IvyExec Weekly Report Maker",
 "version": "1.0",
 "content_security_policy": "script-src 'self' https://cdnjs.cloudflare.com https://docs.google.com ; object-src 'self'",
 "permissions": [
  "tabs",
  "activeTab",
  "storage",
  "contextMenus",
  "https://*/"
 ],
 "browser_action": {
  "default_icon": "images/ivyIconGreen16.png",
  "default_popup": "popup.html"
},
"background": {
  "scripts": ["background.js"],
  "persistant": false
},
 "icons": {
  "16": "images/ivyIconGreen16.png",
  "32": "images/ivyIconGreen32.png",
  "48": "images/ivyIconGreen48.png",
  "128": "images/ivyIconGreen128.png"
},
 "manifest_version": 2
}